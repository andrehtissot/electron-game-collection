    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
    "signcode": "^1.0.0",
    "source-map-loader": "^0.2.4",
    "spectron": "^10.0.1",
    "standard": "^14.3.3",
    "style-loader": "^1.1.3",
