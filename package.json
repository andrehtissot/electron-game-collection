    "csvtojson": "^2.0.10",
    "csvwriter": "^1.3.0",
    "dexie": "^2.0.4",
    "glob": "^7.1.6",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2",
    "react-window": "^1.8.5",
